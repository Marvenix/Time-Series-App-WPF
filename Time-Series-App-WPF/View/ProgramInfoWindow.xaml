<Window x:Class="Time_Series_App_WPF.View.ProgramInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Time_Series_App_WPF.View"
        mc:Ignorable="d"
        Title="ProgramInfoWindow" Height="300" Width="400">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Text="{DynamicResource ProgramInfo}" VerticalAlignment="Top" Height="230" Width="400" Margin="0,55,0,0" TextAlignment="Center" />
    </Grid>
</Window>
